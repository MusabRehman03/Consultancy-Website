{
  "name": "crenotive",
  "version": "1.0.3",
  "main": "index.js",
  "scripts": {
    "watch:sass": "sass sass/style.scss css/style.css --watch",
    "start": "sass sass/style.scss css/style.css --watch",
    "compile:sass": "sass sass/style.scss css/style.css",
    "prefix:css": "postcss --use autoprefixer -b 'last 10 versions' css/style.css -o css/style.prefix.css",
    "compress:css": "sass css/style.prefix.css css/style.css --style=compressed",
    "build:css": "npm-run-all compile:sass prefix:css compress:css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MusabRehman03/Portfolio-Musab"
  },
