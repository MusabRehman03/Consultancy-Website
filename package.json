  },
  "homepage": "",
  "devDependencies": {
    "autoprefixer": "^10.3.6",
    "concat": "^1.0.3",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.3.8",
    "postcss-cli": "^9.0.1",
    "sass": "^1.89.2"
  },
